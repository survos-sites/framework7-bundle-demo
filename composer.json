{
    "name": "survos-sites/vt",
    "description": "Voxitour, a PWA audio tour",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "bizkit/versioning-bundle": "^1.2.1",
        "doctrine/dbal": "^4.3.3",
        "doctrine/doctrine-bundle": "^2.16.1",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.2",
        "endroid/qr-code-bundle": "^6.0",
        "friendsofsymfony/jsrouting-bundle": "^3.5.2",
        "inspector-apm/inspector-symfony": "^1.8.4",
        "knplabs/knp-menu-bundle": "^3.6",
        "league/commonmark": "^2.7.1",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpstan/phpdoc-parser": "^2.3",
        "spomky-labs/pwa-bundle": "1.3.x-dev",
        "survos/auth-bundle": ">=1.6.44",
        "survos/command-bundle": "^1.6.44",
        "survos/crawler-bundle": ">=1.6.44",
        "survos/fw-bundle": "^2.0.5",
        "survos/js-twig-bundle": "dev-main",
        "symfony/asset": "^7.3",
        "symfony/asset-mapper": "^7.3.3",
        "symfony/console": "7.3.x",
        "symfony/doctrine-messenger": "^7.3.3",
        "symfony/dotenv": "^7.3.2",
        "symfony/expression-language": "^7.3.2",
        "symfony/flex": "^2.8.2",
        "symfony/form": "^7.3.3",
        "symfony/framework-bundle": "^7.3.3",
        "symfony/http-client": "^7.3.3",
        "symfony/intl": "^7.3.3",
        "symfony/mailer": "^7.3.3",
        "symfony/mime": "^7.3.2",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^7.3.3",
        "symfony/process": "^7.3.3",
        "symfony/property-access": "^7.3.3",
        "symfony/property-info": "^7.3.1",
        "symfony/runtime": "^7.3.1",
        "symfony/security-bundle": "^7.3.3",
        "symfony/serializer": "^7.3.3",
        "symfony/stimulus-bundle": "^2.30.0",
        "symfony/string": "^7.3.3",
        "symfony/translation": "^7.3.3",
        "symfony/twig-bundle": "^7.3.2",
        "symfony/ux-icons": "^2.30",
        "symfony/ux-leaflet-map": "^2.30.0",
        "symfony/ux-map": "^2.30.0",
        "symfony/ux-twig-component": "^2.30.0",
        "symfony/validator": "^7.3.3",
        "symfony/web-link": "^7.3",
        "symfony/yaml": "^7.3.3",
        "symfonycasts/verify-email-bundle": "^1.17.4",
        "twig/extra-bundle": "^2.12|^3.21",
        "twig/intl-extra": "^3.21",
        "twig/markdown-extra": "^3.21",
        "twig/twig": "^3.21.1"
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": true,
            "php-http/discovery": true,
            "survos/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "fos:js-routing:dump --format=js --target=public/js/fos_js_routes.js --callback=\"export default \"": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "^7.3"
        }
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.4",
        "nikic/php-parser": "^5.6.1",
        "phpunit/phpunit": "^12.3.8",
        "survos/deployment-bundle": "^1.6.44",
        "survos/maker-bundle": "^1.6.44",
        "symfony/browser-kit": "^7.3.2",
        "symfony/css-selector": "^7.3",
        "symfony/debug-bundle": "^7.3",
        "symfony/maker-bundle": "^1.64.0",
        "symfony/panther": "^2.2",
        "symfony/phpunit-bridge": "^7.3.3",
        "symfony/stopwatch": "^7.3.0",
        "symfony/web-profiler-bundle": "^7.3.3",
        "zenstruck/browser": "^1.9.1"
    }
}
